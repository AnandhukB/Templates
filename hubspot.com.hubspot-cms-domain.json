{
  "providerId": "hubspot.com",
  "providerName": "HubSpot",
  "serviceId": "hubspot-cms-domain",
  "serviceName": "HubSpot CMS Site",
  "syncPubKeyDomain": "domainconnect.hubspot.com",
  "syncRedirectDomain": "app.hubspot.com, app.hubspotqa.com, api.hubspot.com, api.hubspotqa.com",
  "version": 1,
  "logoUrl": "https://domainconnect.org/wp-content/uploads/2017/02/HubSpot-768x223.png",
  "description": "Enables a domain to work with the HubSpot CMS",
  "variableDescription": "IPs: HubSpot CMS IPs, txtValue - txt data value, sslTxtValue - ssl txt validation value, cnameValue - cname record pointsTo value",
  "records": [{
    "groupId":"a1",
    "type":"A",
    "host":"@",
    "pointsTo":"%IP1%",
    "ttl":600
  }, {
    "groupId":"a2",
    "type":"A",
    "host":"@",
    "pointsTo":"%IP2%",
    "ttl":600
  },
    {
      "groupId": "txt",
      "type": "TXT",
      "host": "_cf-custom-hostname",
      "data": "%txtValue%",
      "ttl": 3600
    },
    {
      "groupId": "ssl-txt",
      "type": "TXT",
      "host": "_acme-challenge",
      "data": "%sslTxtValue%",
      "ttl": 3600
    }
  ]
}
