{
  "providerId": "secureserver.net",
  "providerName": "SecureServer",
  "serviceId": "professional_mail",
  "serviceName": "Professional Mail",
  "version": 2,
  "description": "Enables a domain to work with Whitelabel Professional Mail",
  "shared": true,
  "sharedProviderName": true,
  "variableDescription": "txtValue - txt record data value, dmarcPolicy - the policy for handling dmarc failures, dkimCname1/2 - the zone partition for the user domain",
  "syncPubKeyDomain": "domain-attach.api.godaddy.com",
  "records": [
    {
      "groupId": "service",
      "type": "MX",
      "host": "@",
      "pointsTo": "smtp.secureserver.net",
      "priority": 0,
      "ttl": 3600
    },
    {
      "groupId": "service",
      "type": "MX",
      "host": "@",
      "pointsTo": "mailstore1.secureserver.net",
      "priority": 10,
      "ttl": 3600
    },
    {
      "groupId": "service",
      "type": "SPFM",
      "host": "@",
      "spfRules": "include:secureserver.net"
    },
    {
      "groupId": "service",
      "type": "CNAME",
      "host": "email",
      "pointsTo": "email.secureserver.net",
      "ttl": 3600
    },
    {
      "groupId": "service",
      "protocol": "_tcp",
      "name": "@",
      "weight": 0,
      "target": "autodiscover.secureserver.net",
      "priority": 0,
      "service": "_autodiscover",
      "type": "SRV",
      "port": 443,
      "ttl": 3600
    },
    {
      "groupId": "verification",
      "type": "TXT",
      "host": "@",
      "data": "%txtValue%",
      "ttl": 3600
    },
    {
      "groupId": "dmarc",
      "type": "TXT",
      "txtConflictMatchingPrefix": "v=DMARC1;",
      "host": "_dmarc",
      "data": "v=DMARC1; p=~dmarcPolicy~; rua=mailto:dmarc_rua@onsecureserver.net;",
      "ttl": 3600
    },
    {
      "groupId": "dkim",
      "type": "CNAME",
      "host": "secureserver_s1._domainkey",
      "pointsTo": "s1.dkim.%dkimCname1%.onsecureserver.net",
      "ttl": 3600
    },
    {
      "groupId": "dkim",
      "type": "CNAME",
      "host": "secureserver_s2._domainkey",
      "pointsTo": "s1.dkim.%dkimCname2%.onsecureserver.net",
      "ttl": 3600
    }
  ]
}
